{
  "name": "generator-integrity",
  "version": "1.1.4",
  "description": "A Next.js + Prisma + Cypress project generator using current tools and frameworks at Integrity Web Consulting.",
  "author": "Integrity Web Consulting",
  "repository": {
    "type": "git",
    "url": "https://github.com/bpkennedy/generator-integrity.git"
  },
  "type": "commonjs",
  "scripts": {
    "build": "webpack",
    "test": "mocha test/content.js --timeout 8000",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prepush": "yarn lint && yarn test",
    "prepublish": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prepush"
    }
  },
  "keywords": [
    "yeoman-generator"
  ],
  "license": "UNLICENSED",
  "files": [
    "generators"
